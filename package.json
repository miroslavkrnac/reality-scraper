{
	"name": "reality-scraper",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start -p 3005",
		"cron": "node cron.js",
		"lint": "biome check",
		"lint:fix": "biome check --fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"db:up": "docker-compose up -d postgres",
		"db:down": "docker-compose down",
		"db:logs": "docker-compose logs postgres",
		"db:push": "prisma db push",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:seed": "tsx prisma/seed.ts",
		"scrape": "tsx test/scraper.ts",
		"docker:build": "docker build --platform linux/amd64 -t registry.gitlab.com/krnacmir/reality-scraper .",
		"docker:push": "docker push registry.gitlab.com/krnacmir/reality-scraper",
		"release": "yarn docker:build && yarn docker:push",
		"antcss": "node ./scripts/genAntdCss.mjs"
	},
	"dependencies": {
		"@ant-design/cssinjs": "^1.24.0",
		"@ant-design/static-style-extract": "^1.0.3",
		"@prisma/client": "^5.8.1",
		"antd": "^5.13.2",
		"next": "15",
		"node-cron": "^4.2.1",
		"puppeteer": "^24.16.0",
		"react": "^18.3.0",
		"react-dom": "^18.3.0",
		"swr": "^2.2.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/react": "^14.1.2",
		"@types/node": "^22",
		"@types/node-cron": "^3.0.11",
		"@types/react": "^18.3.0",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^5.0.0",
		"jsdom": "^23.2.0",
		"knip": "^5.62.0",
		"prisma": "^5.8.1",
		"sass": "^1.69.7",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2",
		"vitest": "^1.2.0"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	}
}
